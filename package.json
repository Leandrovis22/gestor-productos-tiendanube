{
  "name": "gestor-productos-tiendanube",
  "version": "1.0.0",
  "description": "Automatizador para productos de TiendaNube",
  "main": "main.js",
  "scripts": {
    "start": "concurrently \"npm run start:renderer\" \"wait-on http://localhost:5173 && electron .\"",
    "start:renderer": "cd renderer && npm run dev",
    "build": "cd renderer && npm run build",
    "build:electron": "npm run build && electron-builder",
    "dev": "npm run start"
  },
  "dependencies": {
    "concurrently": "^7.0.0",
    "electron": "^26.0.0",
    "fs-extra": "^11.3.2",
    "opencv.js": "^1.2.1",
    "papaparse": "^5.4.1",
    "sharp": "^0.33.5",
    "wait-on": "^7.0.0"
  },
  "devDependencies": {
    "electron-builder": "^24.0.0"
  }
}
